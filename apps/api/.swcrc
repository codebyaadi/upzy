{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "target": "es2022",
    "baseUrl": "./"
  },
  "module": {
    "type": "es6",
    "importInterop": "node",
    "noInterop": false
  },
  "minify": false
}
