{
  "name": "@upzy/lib",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@upzy/eslint-config": "workspace:*",
    "@upzy/typescript-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "5.7.3"
  },
  "main": "./dist/index.js",
  "types": "./src/index.ts",
